<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="anchorPaneBackground" prefHeight="100.0" prefWidth="255.0" style="-fx-background-color: #8d6262;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SettingsController">
   <children>
      <HBox fx:id="hBoxSettings" onMouseClicked="#hBoxSettingsClicked" prefHeight="100.0" prefWidth="275.0">
         <children>
            <ImageView fx:id="imSettingsLogout" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="14.0" top="50.0" />
               </HBox.margin>
               <image>
                  <Image url="@../icons/sign_out.png" />
               </image>
            </ImageView>
            <Label fx:id="lblSettingsLogout" prefHeight="35.0" prefWidth="275.0" text="Logout">
               <font>
                  <Font name="Segoe UI" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" top="45.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
